{
  "name": "copilot-pro",
  "productName": "Copilot Pro",
  "version": "1.0.0",
  "description": "Advanced Offline AI Copilot Pro - Professional Grade AI Coding Assistant",
  "main": "electron.js",
  "homepage": "https://github.com/SpidermanTotro/AgentFoundry-instantly",
  "author": {
    "name": "Copilot Pro Team",
    "email": "support@copilotpro.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpidermanTotro/AgentFoundry-instantly.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run server\" \"wait-on http://localhost:3001 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:all": "npm run build && electron-builder -mwl",
    "docker:build": "docker build -t copilot-pro:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "build": {
    "appId": "com.copilotpro.app",
    "productName": "Copilot Pro",
    "copyright": "Copyright Â© 2024 Copilot Pro Team",
    "directories": {
      "output": "dist-electron",
      "buildResources": "public"
    },
    "files": [
      "dist/**/*",
      "server/**/*",
      "public/**/*",
      "electron.js",
      "preload.js",
      "package.json",
      "index.html"
    ],
    "extraResources": [
      {
        "from": "server/ai-engine",
        "to": "server/ai-engine",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "node_modules/@tensorflow/**/*"
    ],
    "npmRebuild": false,
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development",
      "icon": "public/icon.png",
      "maintainer": "Copilot Pro Team <support@copilotpro.dev>",
      "vendor": "Copilot Pro",
      "synopsis": "Advanced Offline AI Coding Assistant",
      "description": "ChatGPT 2.0 UNRESTRICTED - Professional-grade AI coding assistant with zero restrictions. Features include unlimited chat, web browsing, multi-modal AI (image/video/audio generation), code execution, file system access, and complete offline mode."
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Copilot Pro"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.developer-tools",
      "icon": "public/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "title": "Copilot Pro ${version}",
      "icon": "public/icon.icns"
    },
    "publish": {
      "provider": "github",
      "owner": "SpidermanTotro",
      "repo": "AgentFoundry-instantly"
    }
  },
  "keywords": [
    "ai",
    "copilot",
    "code-assistant",
    "programming",
    "offline",
    "ide",
    "development",
    "code-intelligence",
    "refactoring",
    "code-analysis"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-python": "^6.2.1",
    "@esbuild/linux-x64": "^0.27.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/generative-ai": "^0.24.1",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@rollup/rollup-linux-x64-gnu": "^4.53.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@xenova/transformers": "^2.17.2",
    "acorn": "^8.15.0",
    "ai": "^5.0.112",
    "anthropic": "^0.0.0",
    "ast-types": "^0.16.1",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "body-parser": "^2.2.1",
    "brain.js": "^2.0.0-beta.24",
    "canvas": "^3.2.0",
    "cheerio": "^1.1.2",
    "chokidar": "^5.0.0",
    "codemirror": "^6.0.2",
    "cohere": "^1.1.1",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "diff": "^8.0.2",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "elevenlabs": "^1.59.0",
    "eslint": "^9.39.1",
    "esprima": "^4.0.1",
    "express": "^5.2.1",
    "fast-glob": "^3.3.3",
    "ffmpeg-static": "^5.3.0",
    "flexsearch": "^0.8.212",
    "fluent-ffmpeg": "^2.1.3",
    "fuse.js": "^7.1.0",
    "gifshot": "^0.4.5",
    "git-diff-parser": "^1.0.0",
    "glob": "^13.0.0",
    "highlight.js": "^11.11.1",
    "jimp": "^1.6.0",
    "jscodeshift": "^17.3.0",
    "langchain": "^1.1.5",
    "level": "^10.0.0",
    "lokijs": "^1.5.12",
    "lowdb": "^7.0.1",
    "lunr": "^2.3.9",
    "mammoth": "^1.11.0",
    "markdown-it": "^14.1.0",
    "marked": "^17.0.1",
    "minisearch": "^7.2.0",
    "monaco-themes": "^0.4.7",
    "nanoid": "^5.1.6",
    "natural": "^8.1.0",
    "nedb": "^1.8.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "node-gyp": "^12.1.0",
    "nodemailer": "^7.0.11",
    "openai": "^6.10.0",
    "openai-edge": "^1.2.2",
    "parse-diff": "^0.11.1",
    "pdf-parse": "^2.4.5",
    "pouchdb": "^9.0.0",
    "prettier": "^3.7.4",
    "prism-react-renderer": "^2.4.1",
    "puppeteer": "^24.33.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "recast": "^0.23.11",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "replicate": "^1.4.0",
    "rxdb": "^16.21.1",
    "sanitize-html": "^2.17.0",
    "serpapi": "^2.2.1",
    "sharp": "^0.34.5",
    "short-uuid": "^6.0.3",
    "simple-git": "^3.30.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "striptags": "^3.2.0",
    "tesseract.js": "^6.0.1",
    "tone": "^15.1.22",
    "tree-sitter": "^0.25.0",
    "tree-sitter-javascript": "^0.25.0",
    "typescript-parser": "^2.6.1",
    "uuid": "^13.0.0",
    "validator": "^13.15.23",
    "ws": "^8.18.3",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "vite": "^7.2.7"
  }
}
