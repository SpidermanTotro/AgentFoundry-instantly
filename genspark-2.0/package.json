{
  "name": "genspark-2.0",
  "version": "2.0.0",
  "description": "GenSpark 2.0 - Complete Unified AI Platform with ALL AgentFoundry Features",
  "main": "electron/main.js",
  "author": "GenSpark Team",
  "license": "MIT",
  "homepage": "https://github.com/SpidermanTotro/AgentFoundry-instantly",
  "scripts": {
    "start": "node src/unified-server.js",
    "start:original": "node src/server.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node src/unified-server.js",
    "server:dev": "nodemon src/unified-server.js",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run server\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:build:appimage": "npm run build && electron-builder --linux AppImage",
    "electron:build:deb": "npm run build && electron-builder --linux deb",
    "electron:build:rpm": "npm run build && electron-builder --linux rpm",
    "electron:build:all": "npm run build && electron-builder --linux AppImage deb rpm",
    "unified": "concurrently \"npm run server\" \"npm run dev\""
  },
  "build": {
    "appId": "com.genspark.v2",
    "productName": "GenSpark 2.0",
    "copyright": "Copyright Â© 2024 GenSpark Team",
    "directories": {
      "output": "dist-electron",
      "buildResources": "public"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "src/server.js",
      "src/ai/**/*",
      "src/workspace/**/*",
      "src/media/**/*",
      "src/offline/**/*",
      "package.json"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Development",
      "icon": "public/icon.png",
      "maintainer": "GenSpark Team",
      "synopsis": "Complete Offline AI Platform",
      "description": "GenSpark 2.0 - AI Workspace, Media Generation, Code Completion, and More"
    }
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@huggingface/inference": "^2.6.4",
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "body-parser": "^2.2.1",
    "canvas": "^2.11.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "gifencoder": "^2.0.1",
    "highlight.js": "^11.11.1",
    "html-to-image": "^1.11.11",
    "mammoth": "^1.6.0",
    "markdown-it": "^13.0.2",
    "marked": "^17.0.1",
    "monaco-themes": "^0.4.7",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.13.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.8.1",
    "wavefile": "^11.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.0.2",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  }
}
